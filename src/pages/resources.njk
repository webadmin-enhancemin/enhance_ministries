<!-- ===================== BLOG ===================== -->
<section id="blog" class="section">
    <div class="container">
        <div class="section-header">
            <h2>Blog</h2>
            <p class="section-subtitle">Encouragement, insights, and resources for ministry leaders.</p>
        </div>
        {% set posts = collections.blog | reverse %}
        {% if posts and posts.length %}
        <div class="blog-carousel-wrapper">
            <button class="carousel-btn carousel-btn-prev" aria-label="Previous">&larr;</button>
            <div class="blog-carousel">
                {% for post in posts %}
                <article class="blog-card">
                    {% if post.data.featuredImage %}
                    <div class="blog-card-image">
                        <img src="{{ post.data.featuredImage }}" alt="{{ post.data.title }}" loading="lazy">
                    </div>
                    {% endif %}
                    <div class="blog-card-content">
                        {% if post.data.category %}
                        <span class="event-badge blog-card-category">{{ post.data.category }}</span>
                        {% endif %}
                        <h3 class="blog-card-title"><a href="{{ post.url }}">{{ post.data.title }}</a></h3>
                        <p class="blog-card-meta">{{ post.data.date | readableDate }}{% if post.data.author %} &middot; {{ post.data.author }}{% endif %}</p>
                        <p class="blog-card-excerpt">{{ post.data.description }}</p>
                        <a href="{{ post.url }}" class="btn btn-primary btn-sm">Read More</a>
                    </div>
                </article>
                {% endfor %}
            </div>
            <button class="carousel-btn carousel-btn-next" aria-label="Next">&rarr;</button>
            <div class="carousel-hint">Swipe to see more</div>
        </div>
        {% else %}
        <p class="blog-empty">No posts yet. Check back soon!</p>
        {% endif %}
    </div>
</section>

